%h2= "Title \"#{@title.to_param}\""
- content_tag_for :div, @title do
  %ul{:class => 'attributes'}
    %li
      %strong Name:
      =h @title.name
    %li
      %strong Language:
      =h @title.language.name
%ul{:class => 'menu'}
  %li= link_to 'Edit', edit_title_path(@title)
  %li= link_to 'Back', titles_path